<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<typeAliases>
		<typeAlias alias="studentModel" type="com.project.model.Student" />		
		<typeAlias alias="addressModel" type="com.project.model.Address" />
		<typeAlias alias="barcodeModel" type="com.project.model.Barcode" />
 		<typeAlias alias="contractModel" type="com.project.model.Contract"/> 
	</typeAliases>

	<mappers>
		<mapper resource="mybatis/mapper/student.xml" />
		<mapper resource="mybatis/mapper/Barcode.xml" />
		<mapper resource="mybatis/mapper/Contract.xml" /> 
	</mappers>

</configuration>
